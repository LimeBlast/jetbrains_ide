          <IdeaActionID>org.jetbrains.plugins.ruby.rails.actions.generators.GeneratorsPopupAction</IdeaActionID>
          <ShortCut>'⌥⌘G'</ShortCut>
          <Description>Run Rails Generator...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Submit">
        <Item>
          <IdeaActionID>Console.TableResult.Submit</IdeaActionID>
          <ShortCut>'⌘⏎'</ShortCut>
